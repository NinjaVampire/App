<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App.ContentPages.Nav">
    <ContentPage.ToolbarItems>
        <ToolbarItem Text="ContentPage1" Order="Secondary" Clicked="ToolbarItem_Clicked"/>
        <ToolbarItem Text="ContentPage2" Order="Secondary" Clicked="ToolbarItem_Clicked_1"/>
        <ToolbarItem Text="TabbedPage" Order="Secondary" Clicked="ToolbarItem_Clicked_2"/>
        <ToolbarItem Text="CarouselPage" Order="Secondary" Clicked="ToolbarItem_Clicked_3"/>
    </ContentPage.ToolbarItems>
    
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Nav1"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>